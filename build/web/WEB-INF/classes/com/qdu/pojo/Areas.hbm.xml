<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.qdu.pojo.Areas" table="Areas" schema="dbo" catalog="AntExpress" optimistic-lock="version">
        <id name="rid" type="int">
            <column name="RID" />
            <generator class="assigned" />
        </id>
        <property name="province" type="string">
            <column name="Province" not-null="true" />
        </property>
        <property name="city" type="string">
            <column name="City" not-null="true" />
        </property>
        <property name="district" type="string">
            <column name="District" not-null="true" />
        </property>
        <property name="postCode" type="int">
            <column name="PostCode" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
