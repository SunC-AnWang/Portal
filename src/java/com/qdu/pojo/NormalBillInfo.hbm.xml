<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.qdu.pojo.NormalBillInfo" table="NormalBillInfo" schema="dbo" catalog="AntExpress" optimistic-lock="version">
        <id name="nid" type="int">
            <column name="NID" />
            <generator class="assigned" />
        </id>
        <property name="did" type="int">
            <column name="DID" not-null="true" />
        </property>
        <property name="other" type="string">
            <column name="Other" />
        </property>
        <property name="pickupTime" type="string">
            <column name="PickupTime" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
