<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.qdu.pojo.Departments" table="Departments" schema="dbo" catalog="AntExpress" optimistic-lock="version">
        <id name="deId" type="int">
            <column name="DeId" />
            <generator class="assigned" />
        </id>
        <property name="edepartment" type="string">
            <column name="EDepartment" />
        </property>
        <property name="other" type="string">
            <column name="Other" />
        </property>
    </class>
</hibernate-mapping>
